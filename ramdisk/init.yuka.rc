on late-fs
    # boot time fs tune
    write /sys/block/sda/queue/iostats 0
    write /sys/block/sda/queue/scheduler maple
    write /sys/block/sda/queue/iosched/slice_idle 0
    write /sys/block/sda/queue/read_ahead_kb 512
    write /sys/block/sda/queue/nr_requests 256
    write /sys/block/sde/queue/iostats 0
    write /sys/block/sde/queue/scheduler maple
    write /sys/block/sde/queue/iosched/slice_idle 0
    write /sys/block/mmcblk0/queue/read_ahead_kb 2048
    write /sys/block/sde/queue/nr_requests 256
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1497600
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 422400
on boot
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1497600
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 422400
    write /sys/block/mmcblk0/queue/read_ahead_kb 2048
on property:sys.boot_completed=1

    # Set default profile
    write /sys/devices/system/cpu/cpu0/online 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 422400
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1497600

    write /sys/devices/system/cpu/cpu4/online 1
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor "schedutil"
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 422400
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1209600

    write /sys/class/kgsl/kgsl-3d0/devfreq/max_freq 450000000
    write /sys/class/kgsl/kgsl-3d0/max_gpuclk 216000000
    write /sys/class/kgsl/kgsl-3d0/default_pwrlevel 4
    write /sys/class/kgsl/kgsl-3d0/devfreq/governor msm-adreno-tz
    write /proc/sys/kernel/sched_boost 0

    # according to Qcom this legacy value improves first launch latencies
    # stock value is 512k
    setprop dalvik.vm.heapminfree 2m

    # end boot time fs tune
    write /sys/block/sda/queue/read_ahead_kb 1024
    write /sys/block/sda/queue/nr_requests 128
    write /sys/block/sda/queue/iostats 1
    write /sys/block/mmcblk0/queue/read_ahead_kb 2048
    write /sys/block/sde/queue/nr_requests 128
    write /sys/block/sde/queue/iostats 1
    # write /sys/devices/soc/c175000.i2c/i2c-1/1-0020/input/input2/wake_gesture 1
	
	# Low memory killer
    write /sys/module/lowmemorykiller/parameters/minfree "9466,14199,28398,47330,66262,70995"

    

    
    exec u:r:init:s0 -- /init.yukapower.sh
    exec u:r:su:s0 root root -- /init.yukapower.sh
    exec u:r:supersu:s0 root root -- /init.yukapower.sh
